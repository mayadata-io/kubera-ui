{
  "name": "kubera-ui",
  "license": "Apache-2.0",
  "version": "0.1.0",
  "private": false,
  "description": "Component Library for Kubera products",
  "author": "MayaData, Inc.",
  "homepage": "https://github.com/mayadata-io/kubera-ui",
  "bugs": {
    "url": "https://github.com/mayadata-io/kubera-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayadata-io/kubera-ui.git"
  },
  "scripts": {
    "build": "yarn clean; rollup -c; yarn postBuild",
    "postBuild": "node ./scripts/postBuild",
    "clean": "rimraf dist",
    "dev": "yarn storybook",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,js,jsx,md}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,json,js,jsx}\"",
    "lint": "yarn lint:script && yarn lint:style",
    "lint:script": "eslint --ext js --ext jsx --ext ts --ext tsx src",
    "lint:style": "stylelint src",
    "storybook": "start-storybook -p 9001 -s ./src/assets -c .storybook",
    "test": "jest --maxWorkers=2",
    "test:watch": "yarn test --watch",
    "test:coverage": "jest --coverage --colors --maxWorkers=2"
  },
  "lint-staged": {
    "src/**/*.+(js|jsx|ts|tsx)": [
      "yarn lint",
      "yarn format",
      "git add"
    ],
    "src/**/*.+(json|md)": [
      "yarn format"
    ],
    "src/**/*.+(css)": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "main": "lib.cjs.js",
  "module": "lib.esm.js",
  "browser": "lib.umd.js",
  "types": "index.d.ts",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0 || ^4.10.2",
    "react": "^16.13.1 || ^16.9.0",
    "react-dom": "^16.13.1 || ^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@storybook/addon-a11y": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.26",
    "@testing-library/react": "^10.0.2",
    "@types/autoprefixer": "^9.7.2",
    "@types/classnames": "^2.2.10",
    "@types/cssnano": "^4.0.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^10.12.18",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "^25.3.0",
    "lint-staged": "^10.4.0",
    "postcss-easy-import": "^3.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.0",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1"
  }
}
